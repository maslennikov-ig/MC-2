    "dev": "next dev --turbopack",
    "dev:pretty": "next dev --turbopack | pnpm exec pino-pretty",
    "dev:webpack": "next dev",
    "build": "next build",
    "build:clean": "rm -rf .next node_modules/.cache tsconfig.tsbuildinfo && next build",
